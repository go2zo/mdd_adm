<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=help">
         <menu
               label="MDD">
            <command
                  commandId="kr.co.apexsoft.stella.mdd.ui.generateTo"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="kr.co.apexsoft.stella.mdd.ui.generateTo"
            name="Generate To...">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="kr.co.apexsoft.stella.mdd.ui.handler.GenerateToHandler"
            commandId="kr.co.apexsoft.stella.mdd.ui.generateTo">
         <activeWhen>
            <or>
               <with
                     variable="selection">
                  <test
                        forcePluginActivation="true"
                        property="kr.co.apexsoft.stella.modeler.ui.umlTester.isNamedElement"
                        value="true">
                  </test>
               </with>
            </or>
         </activeWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="kr.co.apexsoft.stella.mdd.ui.tester.UMLPropertyTester"
            id="kr.co.apexsoft.stella.mdd.ui.umlTester"
            namespace="kr.co.apexsoft.stella.mdd.ui.umlTester"
            properties="isNamedElement"
            type="org.eclipse.jface.viewers.IStructuredSelection">
      </propertyTester>
   </extension>

</plugin>
